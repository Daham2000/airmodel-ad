        <div id="viewId">
            <div class="d-flex" style="flex-direction: row;">
                <h5>Add New Product</h5>
            </div>
            <form method="post" asp-controller="ManageProductAdmin" asp-action="AddProductOnSubmit">
                <div class="form-group">
                    <label for="exampleInputEmail1">Products Name</label>
                    <input required type="text" class="form-control" name="productName" id="cName" value="@ViewBag.item.productName" aria-describedby="emailHelp" placeholder="Enter product name">
                </div>
                <div class="mb-1">
                    <label>Select category type</label>
                    <select name="categoryName" value="@ViewBag.item.categoryName" class="form-control form-control-sm">
                        @foreach (var type in @ViewBag.categories)
                        {
                            if (@ViewBag.item.categoryName == type.categoryName)
                            {
                                <option selected>@type.categoryName</option>

                            }
                            else
                            {
                                <option>@type.categoryName</option>
                            }
                        }
                    </select>
                </div>
                <div class="mb-1">
                    <label>Select If varient product</label>
                    <select name="hasVarients" value="@ViewBag.hasVarients" class="form-control form-control-sm">
                        @foreach (var booll in @ViewBag.booll)
                        {
                            if (booll == @ViewBag.hasVarients)
                            {
                                <option selected>@booll</option>

                            }
                            else
                            {
                                <option>@booll</option>
                            }
                        }
                    </select>
                </div>
                <div class="form-group">
                    <label for="exampleInputEmail1">Product Genaral Price</label>
                    <input required type="number" class="form-control" name="productBasicPrice" id="productBasicPrice" value="@ViewBag.item.productBasicPrice" aria-describedby="emailHelp" placeholder="Enter product price">
                </div>
                <div class="form-group">
                    <label for="exampleInputEmail1">Product Image URL</label>
                    <input required type="text" class="form-control" name="productImage" id="productBasicPrice" value="@ViewBag.item.productImage" aria-describedby="emailHelp" placeholder="Enter computer image URL">
                </div>
                <div class="form-group">
                    <label for="exampleInputEmail1">Product Description</label>
                    <input required type="text" class="form-control" name="productDescription" id="productDescription" value="@ViewBag.item.productDescription" aria-describedby="emailHelp" placeholder="Enter Product Description">
                </div>
                <div class="form-group">
                    <label for="exampleInputEmail1">Product Quantity</label>
                    <input required type="number" class="form-control" name="productQty" id="productQty" value="@ViewBag.item.productQty" aria-describedby="emailHelp" placeholder="Enter qty">
                </div>
                <div class="dropdown-divider w-50 mb-1"></div>
                <input type="text" style="display: none" class="form-control" name="productId" value="@ViewBag.item.productId">

                <button type="submit" class="btn btn-primary mt-2">Save Changes</button>
            </form>

            <div class="mb-1 mt-2">
                @{
                    if (@ViewBag.item.varientModels != null)
                    {
                        if (@ViewBag.item.hasVarients == true)
                        {
                            <h5>Edit Variation options</h5>
                            @foreach (var varient in @ViewBag.item.varientModels)
                            {
                                <div class="text-sm-lef mb-1 mt-1">
                                    Variation name: @varient.productName
                                    <div class="m-2 ml-3" style="padding-left: 20px;">
                                        <h6>Variation Options</h6>
                                        @foreach (var option in @ViewBag.item.varientOptionModels)
                                        {
                                            if (option.varientId == varient.varientId)
                                            {
                                                if (option.varientOptionId != new Guid("00000000-0000-0000-0000-000000000000"))
                                                {
                                                    <div>
                                                        <form method="post" asp-controller="ManageProductAdmin" asp-action="EditVarientOnSubmit">
                                                            <input type="text" class="form-control mb-1" name="varientName" id="varientName" value="@option.varientName" placeholder="Enter Variation Option Name">
                                                            <input type="number" class="form-control mb-1" name="varientPrice" id="varientPrice" value="@option.varientPrice" aria-describedby="emailHelp" placeholder="Enter Variation Option Price">
                                                            <input type="number" class="form-control mb-1" name="varientQty" id="varientQty" value="@option.varientQty" aria-describedby="emailHelp" placeholder="Enter Variation Option Qty">
                                                            <input type="text" class="form-control mb-1" name="varientImage" id="varientImage" value="@option.varientImage" aria-describedby="emailHelp" placeholder="Enter Variation Option Image">
                                                            <input type="text" style="display: none" class="form-control" name="varientOptionId" value="@option.varientOptionId">
                                                            <input type="text" style="display: none" class="form-control" name="varientId" value="@option.varientId">
                                                            <input type="text" style="display: none" class="form-control" name="productId" value="@ViewBag.item.productId">

                                                            <button type="submit" class="btn btn-success mt-2 mb-2">Edit Variation Option</button>
                                                        </form>
                                                    </div>
                                                }
                                            }
                                        }
                                        <div class="mt-2 mb-2">
                                            <div class="dropdown-divider w-50 mb-1"></div>
                                            <form method="post" asp-controller="ManageProductAdmin" asp-action="AddVarientOnSubmit">
                                                <input type="text" class="form-control mb-1" name="varientName" id="varientName" placeholder="Enter Variation Option Name">
                                                <input type="number" class="form-control mb-1" name="varientPrice" id="varientPrice" aria-describedby="emailHelp" placeholder="Enter Variation Option Price">
                                                <input type="number" class="form-control mb-1" name="varientQty" id="varientQty" aria-describedby="emailHelp" placeholder="Enter Variation Option Qty">
                                                <input type="text" class="form-control mb-1" name="varientImage" id="varientImage" aria-describedby="emailHelp" placeholder="Enter Variation Option Image">
                                                <input type="text" style="display: none" class="form-control" name="varientId" value="@varient.varientId">
                                                <input type="text" style="display: none" class="form-control" name="productId" value="@ViewBag.item.productId">
                                                <button type="submit" class="btn btn-success mt-2 mb-2">Add New Variation Option</button>
                                            </form>
                                        </div>
                                    </div>
                                    <div class="dropdown-divider w-50 mb-1"></div>
                                </div>
                            }
                        }

                    }
                }

            </div>
        </div>

    </main>
    <script src="https://canvasjs.com/assets/script/canvasjs.min.js"></script>
</body>
</html>
